{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "commonjs",
    // "lib": [],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist",
    "tsBuildInfoFile": "./dist/.tsBuildInfoFile",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": false,
    "esModuleInterop": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
